
public class Main{
public static void main(String[] args) {

        int a = 20, b = 10;
        System.out.println("Arithmetic Operators");
        System.out.println("a + b = " + (a + b)); 
        System.out.println("a * b = " + (a - b)); 
        System.out.println("a * b = " + (a * b)); 
        System.out.println("a / b = " + (a / b)); 
        System.out.println("a % b = " + (a % b)); 
        System.out.println();
}
}
